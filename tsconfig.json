{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "preserve",
    "types": [
      "astro/client",
      "@nuxt/types",
      "@prismicio/types",
      "@prismicio/vue",
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "~/*": [
        "./src/*"
      ]
    }
  },
  "exclude": [
    "node_modules"
  ]
}